#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/wilson/tech_journey/HNG_internship/quick_info_api/node_modules/.pnpm/ts-node@10.9.2_@types+node@22.12.0_typescript@5.7.3/node_modules/ts-node/dist/node_modules:/Users/wilson/tech_journey/HNG_internship/quick_info_api/node_modules/.pnpm/ts-node@10.9.2_@types+node@22.12.0_typescript@5.7.3/node_modules/ts-node/node_modules:/Users/wilson/tech_journey/HNG_internship/quick_info_api/node_modules/.pnpm/ts-node@10.9.2_@types+node@22.12.0_typescript@5.7.3/node_modules:/Users/wilson/tech_journey/HNG_internship/quick_info_api/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/wilson/tech_journey/HNG_internship/quick_info_api/node_modules/.pnpm/ts-node@10.9.2_@types+node@22.12.0_typescript@5.7.3/node_modules/ts-node/dist/node_modules:/Users/wilson/tech_journey/HNG_internship/quick_info_api/node_modules/.pnpm/ts-node@10.9.2_@types+node@22.12.0_typescript@5.7.3/node_modules/ts-node/node_modules:/Users/wilson/tech_journey/HNG_internship/quick_info_api/node_modules/.pnpm/ts-node@10.9.2_@types+node@22.12.0_typescript@5.7.3/node_modules:/Users/wilson/tech_journey/HNG_internship/quick_info_api/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/bin-script-deprecated.js" "$@"
else
  exec node  "$basedir/../../dist/bin-script-deprecated.js" "$@"
fi
